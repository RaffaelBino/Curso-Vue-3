 <li\r\n        v-for=\"(task, index) in tasks\"\r\n        :key=\"`${task}-${index}`\"\r\n        @dblclick=\"complete(task)\"\r\n        class=\"task-item\"\r\n        :class=\"{\r\n          'line-through': task.isDone,\r\n        }\"\r\n      >\r\n        {{ task.name }}\r\n\r\n        <button @click=\"remove(task)\">&times;</button>\r\n      </li>\r\n    </ul>\r\n\r\n    <p v-else>Lista de tarefas escondidas</p>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nconst focus = {\r\n  inserted: (el) => {\r\n    el.focus();\r\n  },\r\n};\r\nexport default {\r\n  directives: {\r\n    focus,\r\n  },\r\n  data: () => ({\r\n    currentTask: \"\",\r\n    showList: false,\r\n    tasks: [\r\n      {\r\n        name: \"Fazer o curso\",\r\n        isDone: false,\r\n      },\r\n    ],\r\n  }),\r\n  methods: {\r\n    handleShowHideList() {\r\n      this.showList = !this.showList;\r\n    },\r\n    addTask() {\r\n      this.tasks.push({\r\n        name: this.currentTask,\r\n        isDone: false,\r\n      });\r\n      this.currentTask = \"\";\r\n    },\r\n    complete(task) {\r\n      this.tasks = this.tasks.map((t) => {\r\n        if (t.name === task.name) {\r\n          return { ...t, isDone: !t.isDone };\r\n        }\r\n        return { ...t };\r\n      });\r\n    },\r\n    remove(task) {\r\n      this.tasks = this.tasks.filter((t) => t.name !== task.name);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.line-through {\r\n  text-decoration: line-through;\r\n}\r\n.task-item {\r\n  cursor: pointer;\r\n}\r\n</style>\r\n"]}]}